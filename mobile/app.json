{
  "name": "ILMS.AIMobile",
  "displayName": "ILMS.AI ERP",
  "version": "1.0.0",
  "description": "ILMS.AI ERP Mobile App for iOS and Android",
  "expo": {
    "name": "ILMS.AI ERP",
    "slug": "ilms-erp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0066CC"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ilms.erp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0066CC"
      },
      "package": "com.ilms.erp"
    }
  }
}
